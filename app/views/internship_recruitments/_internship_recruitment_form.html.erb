<%= form_for @internship_recruitment do |f| %>
  <%= render 'shared/error_messages', object: @internship_recruitment %>

  <%= f.label :title %>
  <%= f.text_field :title, class: 'form-control' %>

  <%= f.label :content %>
  <%= f.text_area :content, class: 'form-control' %>
  <%= f.submit "Save" , class: "btn btn-primary" %>
<% end %>